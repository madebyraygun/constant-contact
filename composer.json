{
    "name": "madebyraygun/constant-contact",
    "description": "Basic Contact Contact integration for Craft CMS",
    "type": "craft-plugin",
    "version": "0.1.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "constant contact"
    ],
    "support": {
        "docs": "https://github.com/madebyraygun/constant-contact/blob/master/README.md",
        "issues": "https://github.com/madebyraygun/constant-contact/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dalton Rooney",
            "homepage": "https://madebyraygun.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "classy-org/constantcontact-php-client": "^1.1.0"
    },
    "autoload": {
        "psr-4": {
          "madebyraygun\\constantcontact\\": "src/"
        }
    },
    "extra": {
        "name": "Constant Contact",
        "handle": "constant-contact",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/madebyraygun/constant-contact/master/CHANGELOG.md",
        "components": {
            "constantContactService": "madebyraygun\\constantcontact\\services\\ConstantContactService"
        },
        "class": "madebyraygun\\constantcontact\\ConstantContact"
    }
}
